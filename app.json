{
  "expo": {
    "name": "Check Now",
    "slug": "fatoseguro",
    "version": "1.5.1",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/favicon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#FFFFFF"
      },
      "runtimeVersion": "1.0.0"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "bundleIdentifier": "checknow.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b04b14b-c4df-4cf9-8b20-e9fa88bc68fe"
      }
    },
    "owner": "henrique_rcz",
    "updates": {
      "url": "https://u.expo.dev/0b04b14b-c4df-4cf9-8b20-e9fa88bc68fe"
    }
  }
}
