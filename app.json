{
  "expo": {
    "name": "Check Now",
    "slug": "fatoseguro",
    "version": "2.8.1",
    "runtimeVersion": "2.8",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/favicon.png",
    "splash": {
      "image": "./assets/images/logozinha.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB",
      "dark": {
        "image": "./assets/images/logozinha.png",
        "resizeMode": "contain",
        "backgroundColor": "#111827"
      }
    },
    "newArchEnabled": true,
    "android": {
      "package": "com.checknow.app",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logozinha.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "checknow.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b04b14b-c4df-4cf9-8b20-e9fa88bc68fe"
      }
    },
    "owner": "henrique_rcz",
    "updates": {
      "url": "https://u.expo.dev/0b04b14b-c4df-4cf9-8b20-e9fa88bc68fe"
    }
  }
}
